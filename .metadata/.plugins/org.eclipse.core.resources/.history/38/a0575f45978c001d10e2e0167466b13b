package java0105;

public class CharExample {

	public static void main(String[] args) {
		// int : 4byte : -21억 ~ +21억
		long var1 = 10;               // int 10
		long var2 = 20L;              // L : long 타입 입력
//		long var3 = 100,000,000,000;  // int 1000억 
		long var4 = 100000000000L;    // long 1000억
	}
}
