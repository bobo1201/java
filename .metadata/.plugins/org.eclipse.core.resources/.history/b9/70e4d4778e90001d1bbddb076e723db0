package java0110;

import java.util.Scanner;

public class ex000000 {
	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
		
		while(true) {
			
			int[] scores = new int[] {};
			

					System.out.print("학생수> ");
					int a = sc.nextInt();
					scores = new int[a];
					
					for(int i=0; i<scores.length; i++) {
						System.out.print("scores[" + i + "]: ");				
						scores[i] = sc.nextInt();
					}
		}
	}
}