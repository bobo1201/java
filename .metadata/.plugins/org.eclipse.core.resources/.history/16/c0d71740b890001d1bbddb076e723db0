package java0110_2;

public class computerExample1 {
	public static void main(String[] args) {

		computer1 com = new computer1();
		
		int sum1 = com.sum(1, 2, 3);
		double avg1 = com.avg(1, 2, 3);
		
		System.out.println("sum1 : " + sum1);
		System.out.println("avg1 : " + avg1);
	}
}

// 출력 :
// result1 : 6
// result2 : 15
// result3 : 15
// result4 : 15