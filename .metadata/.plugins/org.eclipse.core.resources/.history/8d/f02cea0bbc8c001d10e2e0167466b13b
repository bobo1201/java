package java0105;

public class OverflowUnderflowExample {
	public static void main(String[] args) {
		
		byte var1 = 125;
		for(int i=0; i<5; i++) {
			var1++;
			System.out.println("var1 : " + var1);
		}
		
		System.out.println("---------------------");
		byte var2 = -125;
		for(int i=0; i<5; i++) {
			var2--;
			System.out.println("var 2 :" + var2);
		}
		
		byte var3 = 0b1111;
				
	}
}

