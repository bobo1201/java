package java0110_2;

public class Calculator {
	
	//필드 선언
	String company = "현대자동차";
	String model;
	String color;
	int maxSpeed;
	
	Calculator(String model){
		this(model, "은색", 250);
	}

	Calculator(String model, String color){
		this(model, color, 250);
	}
	
	Calculator(String model, String color, int maxSpeed){
		this.model = model;
		this.color = color;
		this.maxSpeed = maxSpeed;
	}
}
